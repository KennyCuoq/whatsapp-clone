<div class="container" id="container-dashboard">
  <div class="navbar">
    <%= link_to destroy_user_session_path, method: :delete do %>
      <i class="fas fa-cog icon"></i>
    <% end %>
    <h4>Chats</h4>
    <i class="far fa-edit icon"></i>
  </div>
  <div class="chat-list">
    <% @chats.each do |chat| %>
      <%= render 'chat_card', chat: chat, user: @user %>
    <% end %>
  </div>
</div>
